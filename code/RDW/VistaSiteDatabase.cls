Class RDW.VistaSiteDatabase Extends %Persistent
{

Property VistaSite As RDW.VistaSite;

Property CacheInstance As RDW.CacheInstance;

Property CacheDatabaseName As %String;

Property CacheDatabaseDirectory As %String;

Index MainIndex On VistaSite;

ClassMethod OldInitialize()
{
	Do ..%KillExtent()
	Set sql="SELECT Sta3a,Sta3n,CacheInstance FROM RDW.VistaSite"
	Set tStatement=##class(%SQL.Statement).%New()
	Set Status=tStatement.%Prepare(sql)
	Set rSet=tStatement.%Execute()
	While rSet.%Next() {
		For db="rou","vaa","vbb","vcc","vdd","vee","vff","vgg","vhh","vii","vjj","vkk" {
		Set sta3a=rSet.Sta3a
		Set sta3n=rSet.Sta3n
		Set inst=rSet.CacheInstance
		Set dbDir="/cachedb/v"_$Extract((100+dc),2,3)_"/"_sta3a_"/"_db
		Set dbName=sta3a_"-"_db
		&sql(INSERT INTO RDW.VistaSiteDatabase(VistaSite,CacheInstance,CacheDatabaseName,CacheDatabaseDirectory) VALUES(:sta3n,:inst,:dbName,:dbDir))
		}
	}
	Do tStatement.%Close()
	Do rSet.%Close()
	
}

